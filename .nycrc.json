{
    "cache": false,
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "extension": [
        ".ts"
    ],
    "include": [
        "**/src/**/*.ts"
    ],
    "exclude": [
        "**/src/contracts/*"
    ],
    "reporter": [
        "cobertura",
        "text-summary"
    ],
    "statements": 90,
    "branches": 90,
    "functions": 90,
    "lines": 90
}